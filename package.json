{
  "name": "@cvarjao/rlm",
  "version": "1.0.0-alpha.2",
  "description": "Release Lifecycle Managament - CLI",
  "main": "dist/index.js",
  "bin": "bin.js",
  "files": ["dist/index.js", "bin.js"],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build lib/main.js",
    "test": "jest",
    "clean": "rm -rf lib dist",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test",
    "publish": "cp package.json package.original.json && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cvarjao-o/rlm.git"
  },
  "author": "Clecio Varjao",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cvarjao-o/rlm/issues"
  },
  "homepage": "https://github.com/cvarjao-o/rlm#readme",
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.21.1",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "bunyan": "^1.8.12"
  },
  "bundledDependencies":["bunyan"]
}
